    "version": "0.2.0",
    "configurations": [
        {
            "name": "CUDA C++: Launch",
            "type": "cuda-gdb",
            "request": "launch",
            "preLaunchTask": "${defaultBuildTask}",
            "program": "${workspaceFolder}/build/hello"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
        },
        {
            "name": "Python: Ins",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/ins/run.py",
            "console": "integratedTerminal",
            "justMyCode": true,
        },
    ]
}
